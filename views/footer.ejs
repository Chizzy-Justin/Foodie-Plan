  <script>
    var quill = new Quill('#quillEditor', {
        theme: 'snow',
        modules: {
            toolbar: [
                ['bold', 'italic'],
                [{'list': 'ordered'}, {'list': 'bullet'}],
                [{'align': []}],
                ['clean']
            ]
        }
    });

    var form = document.getElementById('FoodNoteForm');
    form.onsubmit = function() {
        var description = document.querySelector('input[name=FoodNote]');
        description.value = quill.root.innerHTML;
    };
</script>
  <script>
   
    const notes = <%-JSON.stringify(notes)%>;

      function highlightDates() { 
         
          const datePickers = document.querySelectorAll('.date-picker'); 
          datePickers.forEach(cell => { 
              const cellDate = parseInt(cell.getAttribute('data-date')); 
              const cellMonth = parseInt(cell.getAttribute('data-month')); 
              const cellYear = parseInt(cell.getAttribute('data-year')); 
              
              notes.forEach(note => { 
                  const noteDate = new Date(note.created_at);
                  console.log( 'noteDate', noteDate.getDate());
                  
                   if (noteDate.getFullYear() == cellYear && (noteDate.getMonth() + 1) == cellMonth && noteDate.getDate() == cellDate) { 
  
                       cell.classList.add('highlight'); 
                      }
                   }); 
                  });
               }
  
                         highlightDates();
  </script>


